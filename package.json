{
  "name": "plundora-backend",
  "version": "1.0.0",
  "description": "Backend API for Plundora marketplace platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "node scripts/migrate.js",
    "railway-migrate": "node scripts/railway-migrate.js",
    "simple-migrate": "node scripts/simple-migrate.js",
    "check-env": "node scripts/check-env.js",
    "setup": "npm install && npm run check-env && npm run migrate",
    "deploy-setup": "node scripts/deploy-setup.js",
    "health": "curl http://localhost:3001/health || node -e \"console.log('Server not running or health check failed')\"",
    "railway-debug": "node scripts/railway-debug.js",
    "test-api": "node scripts/test-api.js",
    "test-database": "node scripts/test-database.js",
    "migrate-direct": "node scripts/railway-migrate-direct.js"
  },
  "keywords": [
    "marketplace",
    "api",
    "express",
    "postgresql"
  ],
  "author": "Plundora Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "stripe": "^14.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}